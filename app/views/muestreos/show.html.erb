<p id="notice"><%= notice %></p>

<p>
  <strong>Fecha inicio:</strong>
  <%= @muestreo.fecha_inicio %>
</p>

<p>
  <strong>Fecha fin:</strong>
  <%= @muestreo.fecha_fin %>
</p>

<p>
  <strong>Meta:</strong>
  <%= @muestreo.meta %>
</p>

<p>
  <strong>Proyecto:</strong>
  <%= @muestreo.proyecto_id %>
</p>

<p>
	<strong>Horas de muestra</strong>
</p>
<table>
	<thead>
	<tr>
	<th> Dia </th>
	<th> Hora </th>
	</tr>
	</thead>
<% @horarios.each do |sc| %>
	<tbody>
	<tr>
		<td><%= sc.hora.strftime('%F') %></td>
		<td><%= sc.hora.strftime('%I:%M:%S %p') %></td>
	</tr>
	
	</tbody>
<% end %>
</table>
<br>
<%= link_to 'Edit', edit_muestreo_path(@muestreo) %> |
<%= link_to 'Back', muestreos_path %>
